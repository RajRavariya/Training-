   "source": [
    "def var_x(List):\n",
    "    sum_sq=0\n",
    "    for i in range(len(List)):\n",
    "      sum_sq+=(List[i]-mean(List))**2\n",
    "    var=sum_sq/(len(List)-1)\n",
    "    sd=sqrt(var)\n",
    "    return var,sd\n",
    "    "
   ]
  },
